{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["airbnb-base"],
	"parserOptions": {
		"ecmaVersion": 12
	},
	"rules": {
    "no-await-in-loop": 0,
    "no-console": "warn",
    "default-case": 0,
    "no-param-reassign": "off",
		"camelcase": ["error", {"ignoreDestructuring": true, "properties": "never"}]
  }
}
